{
   "author": "Go-ERP developers",
   "background": {
      "persistent": false,
      "scripts": [ "bg.js" ]
   },
   "browser_action": {
      "default_popup": "options.html",
      "default_title": "Mono-tasking tabs"
   },
   "description": "Improve your productivity by limiting number of tabs you can open.",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Mono-tasking tabs",
   "options_page": "options.html",
   "permissions": [ "tabs" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.8.5"
}
